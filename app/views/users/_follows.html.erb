<% users.each do |user|%>
  <div class='follows-user'>
    <div class='prof-img'>
      <% if user.image? %>
        <%= link_to image_tag(user.image.url, class: 'follows-avatar-img'), user %>
      <% else %>
        <%= link_to image_tag("/assets/default.png", class: 'follows-avatar-img'), user %>
      <% end %>
    </div>
    <div class='prof-content'>
      <div class='prof-name'>
        <%= user.user_name%>
      </div>
      <div class='prof-menu'>
        <%= render "relationships/follow_button", user: user %>
      </div>
    </div>
  </div>
<% end %>