<div class='search-box'>
  <%= search_form_for @search do |f| %>

      <%= f.label :target_site, '鍛えられる部位' %>
      <%= f.collection_select :target_site_eq, @search_ary, :target_site, :target_site, include_blank: '部位を選択してください' %>
    <%= f.label :検索 %>
    <%= f.search_field :title_or_need_or_recommended_target_cont, placeholder: 'キーワードを入力' %>

    <%= f.submit '検索'%>
  <% end %>
</div>
